<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="online.bingzi.internal.mapper.user.UserLoginMapper">
    <select id="queryUserByUser" parameterType="online.bingzi.internal.entity.request.user.UserLogin"
            resultType="online.bingzi.internal.entity.request.user.UserLogin">
        select user, password
        from table_user
        where user = #{user}
          and password = #{password}
    </select>
</mapper>