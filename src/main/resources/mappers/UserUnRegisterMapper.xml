<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="online.bingzi.internal.mapper.user.UserUnRegisterMapper">
    <select id="queryUserByUser" resultType="online.bingzi.internal.entity.request.user.UserUnRegister">
        select *
        from table_user
        where user = #{user}
          and password = #{password}
    </select>
    <delete id="deleteUser" parameterType="online.bingzi.internal.entity.request.user.UserUnRegister">
        delete
        from table_user
        where user = #{user}
    </delete>
</mapper>