<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="online.bingzi.internal.mapper.photo.PhotoQueryMapper">
    <select id="queryPhotoByUser" parameterType="online.bingzi.internal.entity.request.photo.PhotoCreate"
            resultType="online.bingzi.internal.entity.request.photo.Photo">
        select user, uid, photo, image
        from table_photo
        where user = #{user}
    </select>
</mapper>